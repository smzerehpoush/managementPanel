<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns="http://xmlns.jcp.org/xml/ns/persistence"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence
             http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd"
             version="2.1">

    <persistence-unit name="managementPanelJPA" transaction-type="JTA">
        <jta-data-source>managementPanelDataSource</jta-data-source>

        <class>com.nrdc.managementPanel.model.dto.Key</class>
        <class>com.nrdc.managementPanel.model.dto.Privilege</class>
        <class>com.nrdc.managementPanel.model.dto.Role</class>
        <class>com.nrdc.managementPanel.model.dto.RolePrivilege</class>
        <class>com.nrdc.managementPanel.model.dto.System</class>
        <class>com.nrdc.managementPanel.model.dto.SystemVersion</class>
        <class>com.nrdc.managementPanel.model.dto.Token</class>
        <class>com.nrdc.managementPanel.model.dto.User</class>
        <class>com.nrdc.managementPanel.model.dto.UserRole</class>
        <class>com.nrdc.managementPanel.model.dto.SystemUser</class>
        <class>com.nrdc.managementPanel.model.dto.Operation</class>

    </persistence-unit>

</persistence>
