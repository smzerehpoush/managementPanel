<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns="http://xmlns.jcp.org/xml/ns/persistence"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence
             http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd"
             version="2.1">

    <persistence-unit name="managementPanelJPA" transaction-type="JTA">
        <jta-data-source>managementPanelDataSource</jta-data-source>

        <class>com.nrdc.managementPanel.model.Key</class>
        <class>com.nrdc.managementPanel.model.Privilege</class>
        <class>com.nrdc.managementPanel.model.Role</class>
        <class>com.nrdc.managementPanel.model.RolePrivilege</class>
        <class>com.nrdc.managementPanel.model.System</class>
        <class>com.nrdc.managementPanel.model.SystemVersion</class>
        <class>com.nrdc.managementPanel.model.Token</class>
        <class>com.nrdc.managementPanel.model.User</class>
        <class>com.nrdc.managementPanel.model.UserRole</class>
        <class>com.nrdc.managementPanel.model.SystemUser</class>

    </persistence-unit>

</persistence>
